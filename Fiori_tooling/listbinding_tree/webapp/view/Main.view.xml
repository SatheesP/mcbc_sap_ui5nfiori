<mvc:View controllerName="masterclass.ui5.listbindingtree.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Tree items="{tree>/weekdays}" >
                <StandardTreeItem title="{tree>iconEmoji} - {tree>name}"/>
            </Tree>
        </content>
    </Page>
</mvc:View>
